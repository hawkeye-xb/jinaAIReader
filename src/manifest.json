{
  "manifest_version": 3,
  "name": "Jina AI Reader",
  "description": "Base Level Extension",
  "version": "0.0.57",
  "action": {
    "default_popup": "index.html",
    "default_icon": "defaultImage.png"
  },
  "permissions": [
    "declarativeNetRequest",
    "activeTab",
    "storage",
    "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "*://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "scripts/background.js"
  }
}
